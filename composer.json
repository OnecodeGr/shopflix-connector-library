{
    "name": "onecode/shopflix-connector-library",
    "type": "library",
    "description":"Official SHOPFLIX connector library for php projects by Onecode",
    "require": {
        "php": ">=7.3",
        "guzzlehttp/guzzle": ">=6.5 || >=7.5",
        "spyrmp/json-serializer-deserializer": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Onecode\\ShopFlixConnector\\Library\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Onecode\\ShopFlixConnector\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "version": "1.1.0",
    "authors": [
        {
            "name": "Onecode P.C",
            "email": "support@onecode.gr"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9",
        "symfony/var-dumper": "^5.4"
    }
}
